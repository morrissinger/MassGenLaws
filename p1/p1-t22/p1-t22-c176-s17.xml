<?xml version="1.0" encoding="utf-8"?>
<law>
    <structure>
        <unit label="part" identifier="1" order_by="1" level="1">Administration Of The Government</unit>
        <unit label="title" identifier="22" order_by="22" level="2">Corporations</unit>
        <unit label="chapter" identifier="176" order_by="176" level="3">Fraternal Benefit Societies</unit>
    </structure>
    <section_number>17</section_number>
    <catch_line>Benefits</catch_line>
    <order_by>17</order_by>
    <text>
        <section prefix="a">
    	     Death benefits in any form;
        </section>
        <section prefix="b">
    	     Endowment benefits;
        </section>
        <section prefix="c">
    	     Annuity benefits;
        </section>
        <section prefix="d">
    	     Temporary or permanent disability benefits;
        </section>
        <section prefix="e">
    	     Hospital, medical or nursing benefits;
        </section>
        <section prefix="f">
    	     Monument or tombstone benefits to the memory of deceased members not exceeding in any case the sum of three hundred dollars; and
        </section>
        <section prefix="g">
    	     Such other benefits as authorized for life insurers and which are not inconsistent with this chapter.
        </section>
    </text>
</law>