<?xml version="1.0" encoding="utf-8"?>
<law>
    <structure>
        <unit label="part" identifier="3" order_by="3" level="1">Courts, Judicial Officers And Proceedings In Civil Cases</unit>
        <unit label="title" identifier="6" order_by="6" level="2">Costs And Fees</unit>
        <unit label="chapter" identifier="261" order_by="261" level="3">Costs In Civil Actions</unit>
    </structure>
    <section_number>1</section_number>
    <catch_line>Prevailing Party To Recover Costs</catch_line>
    <order_by>1</order_by>
    <text>
        In civil actions the prevailing party shall recover his costs, except as otherwise provided.
    </text>
</law>